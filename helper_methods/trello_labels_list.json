{
  "name": "lista dos labels de um boards",
  "helper_method_key": "trello_labels_list",
  "requests": [ 
    {
      "method_name": "/1/boards/{idBoard}/labels",
      "extra_params": [{"key": "idBoard"}],
      "params": {},
        "mapping": {
        "label": {
          "fields": ["name", "color"],
          "separator": ": "
        },
        "value": {
          "field": "id"
        }
      }  
    }
  ]
}